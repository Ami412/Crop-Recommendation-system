@model myproject.Models.VideoCategory
@{
    Layout = "~/Views/_formlayout.cshtml";
    //  ViewBag.Title = "VideoCategoryEdit";
}
<div class="x_title">
    <h2>VIDEO CATEGORY EDIT </h2>

    <div class="clearfix"></div>
</div>

<form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left" action="/Admin/VideoCategoryUpdate" method="post">
    <div class="form-group">
        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="name">
            CATEGORY NAME:
        </label>
        <div class="col-md-6 col-sm-6 col-xs-12">
            <input type="text" name="CategoryName" required="required" value="@Model.CategoryName" class="form-control col-md-7 col-xs-12">
        </div>
    </div>

    <div class="ln_solid"></div>
    <div class="form-group">
        <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
            <input type="hidden" name="VideoCategoryID" value="@Model.VideoCategoryID" />
            <button type="submit" class="btn btn-success">Submit</button>
        </div>
    </div>
</form>
